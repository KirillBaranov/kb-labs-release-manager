{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@kb-labs/changelog": [
        "packages/changelog/src/index.ts"
      ],
      "@kb-labs/changelog/*": [
        "packages/changelog/src/*"
      ],
      "@kb-labs/core": [
        "../kb-labs-core/src/index.ts"
      ],
      "@kb-labs/core-bundle": [
        "../kb-labs-core/packages/core-bundle/src/index.ts"
      ],
      "@kb-labs/core-bundle/*": [
        "../kb-labs-core/packages/core-bundle/src/*"
      ],
      "@kb-labs/core/*": [
        "../kb-labs-core/src/*"
      ],
      "@kb-labs/plugin-contracts": [
        "../kb-labs-plugin/packages/plugin-contracts/src/index.ts"
      ],
      "@kb-labs/plugin-contracts/*": [
        "../kb-labs-plugin/packages/plugin-contracts/src/*"
      ],
      "@kb-labs/plugin-manifest": [
        "../kb-labs-plugin/packages/manifest/src/index.ts"
      ],
      "@kb-labs/plugin-manifest/*": [
        "../kb-labs-plugin/packages/manifest/src/*"
      ],
      "@kb-labs/release-checks": [
        "packages/release-checks/src/index.ts"
      ],
      "@kb-labs/release-checks/*": [
        "packages/release-checks/src/*"
      ],
      "@kb-labs/release-contracts": [
        "packages/release-contracts/src/index.ts"
      ],
      "@kb-labs/release-contracts/*": [
        "packages/release-contracts/src/*"
      ],
      "@kb-labs/release-core": [
        "packages/release-core/src/index.ts"
      ],
      "@kb-labs/release-core/*": [
        "packages/release-core/src/*"
      ],
      "@kb-labs/shared-cli-ui": [
        "../kb-labs-shared/packages/cli-ui/src/index.ts"
      ],
      "@kb-labs/shared-cli-ui/*": [
        "../kb-labs-shared/packages/cli-ui/src/*"
      ],
      "@kb-labs/shared-command-kit": [
        "../kb-labs-shared/packages/shared-command-kit/src/index.ts"
      ],
      "@kb-labs/shared-command-kit/*": [
        "../kb-labs-shared/packages/shared-command-kit/src/*"
      ]
    }
  }
}
