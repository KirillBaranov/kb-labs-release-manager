{
  "$schema": "https://json.schemastore.org/tsconfig",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@kb-labs/plugin-contracts": [
        "../kb-labs-plugin/packages/plugin-contracts/src/index.ts"
      ],
      "@kb-labs/plugin-contracts/*": [
        "../kb-labs-plugin/packages/plugin-contracts/src/*"
      ],
      "@kb-labs/release-manager-changelog": [
        "packages/release-manager-changelog/src/index.ts"
      ],
      "@kb-labs/release-manager-changelog/*": [
        "packages/release-manager-changelog/src/*"
      ],
      "@kb-labs/release-manager-checks": [
        "packages/release-manager-checks/src/index.ts"
      ],
      "@kb-labs/release-manager-checks/*": [
        "packages/release-manager-checks/src/*"
      ],
      "@kb-labs/release-manager-contracts": [
        "packages/release-manager-contracts/src/index.ts"
      ],
      "@kb-labs/release-manager-contracts/*": [
        "packages/release-manager-contracts/src/*"
      ],
      "@kb-labs/release-manager-core": [
        "packages/release-manager-core/src/index.ts"
      ],
      "@kb-labs/release-manager-core/*": [
        "packages/release-manager-core/src/*"
      ],
      "@kb-labs/sdk": [
        "../kb-labs-sdk/packages/sdk/src/index.ts"
      ],
      "@kb-labs/sdk/*": [
        "../kb-labs-sdk/packages/sdk/src/*"
      ]
    }
  }
}
