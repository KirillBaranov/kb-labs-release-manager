{
  "name": "@kb-labs/release-contracts",
  "version": "0.1.0",
  "description": "Contracts for KB Labs Release Manager plugin.",
  "type": "module",
  "private": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
